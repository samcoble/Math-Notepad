{
  "name": "Math Notepad",
  "description": "Write math freely!",
  "version": "2.1",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "icon/icon-16.png", "128": "icon/icon-128.png" },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage",
    "http://www.wolframalpha.com/",
    "https://www.symbolab.com/",
    "webview"
  ],
  "file_handlers": {
    "text": {
      "types": [
          "text/*"
      ]
    }
  }
}